<section id="navigation">
  <nav class="sideNav">
    <ul>
      <% asideMenu.forEach((item) => { %>
        <%- linkGenerator(item, page) %>
      <% }) %>
    </ul>
  </nav>
</section>

<section id="navigationPhone">
  <nav class="sideNav">
    <button class="icon" onclick="hamburgerToogle()">
      <i class="fa fa-bars fa-lg"></i>
   </button>
    <ul id="links">
      <% asideMenu.forEach((item) => { %>
        <%- linkGenerator(item, page) %>
      <% }) %>
    </ul>
  </nav>
</section>

<section class="sponzors-wrapper">
  <h2>Partners:</h2>
  <section class="sponzors">
      <a href="https://www.profinit.eu/" rel="nofollow">
        <picture class="sponzor-logo"></picture>
          <img src="/images/profinit_logo.png" alt="Profinit logo" width="145px" height="22px">
        </picture>
      </a>
    </section>
  </section>

<script>
function hamburgerToogle() {
  let links = document.getElementById("links");
  if (links.style.display === "flex") {
    links.style.display = "none";
  } else {
    links.style.display = "flex";
  }
}
</script>